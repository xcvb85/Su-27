<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<fx>
		<!--================================= Engine =============================-->
		<engine>
			<name>engine-start-outside</name>
			<mode>once</mode>
			<path>Sounds/engine/Mirage2000_Starting_Engine_OutsideV3.wav</path>
			<condition>
				<property>/controls/engines/engine[0]/starter</property>
				<not>
					<and>
						<property>sim/current-view/internal</property>
						<less-than>
							<property>sim/multiplay/generic/float[0]</property>
							<value>0.1</value>
						</less-than>
					</and>
				</not>
			</condition>
			<orientation>
				<x>1.0</x>
				<y>0.0</y>
				<z>0.0</z>
				<inner-angle>30.0</inner-angle>
				<outer-angle>330.0</outer-angle>
				<outer-gain>0.1</outer-gain>
			</orientation>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</engine>
		<engine>
			<name>engine-start-inside</name>
			<mode>once</mode>
			<path>Sounds/engine/mig21startup.wav</path>
			<condition>
				<property>/controls/engines/engine[0]/starter</property>
				<property>sim/current-view/internal</property>
				<less-than>
					<property>sim/multiplay/generic/float[0]</property>
					<value>0.1</value>
				</less-than>
			</condition>
			<volume>
				<max>0.5</max>
			</volume>
		</engine>
		<engine>
			<name>external-whine</name>
			<path>Sounds/engine/Mirage2000_Engine_Outside.wav</path>
			<mode>looped</mode>
			<condition>
				<greater-than>
					<property>engines/engine[0]/n1</property>
					<value>20</value>
				</greater-than>
				<not>
					<property>engines/engine[0]/out-of-fuel</property>
				</not>
				<not>
					<and>
						<property>sim/current-view/internal</property>
						<less-than>
							<property>sim/multiplay/generic/float[0]</property>
							<value>0.1</value>
						</less-than>
					</and>
				</not>
			</condition>
			<orientation>
				<x>1.0</x>
				<y>0.0</y>
				<z>0.0</z>
				<inner-angle>120.0</inner-angle>
				<outer-angle>240.0</outer-angle>
				<outer-gain>0.2</outer-gain>
			</orientation>
			<pitch>
				<property>engines/engine[0]/n1</property>
				<offset>0.7</offset>
				<factor>.003</factor>
			</pitch>
			<volume>
				<property>engines/engine/n2</property>
				<factor>0.1</factor>
				<min>0.0</min>
				<max>6.0</max>
			</volume>
		</engine>
		<engine>
			<name>internal-whine</name>
			<path>Sounds/engine/mig21whine.wav</path>
			<mode>looped</mode>
			<condition>
				<greater-than>
					<property>engines/engine[0]/n1</property>
					<value>20</value>
				</greater-than>
				<not>
					<property>engines/engine[0]/out-of-fuel</property>
				</not>
				<property>sim/current-view/internal</property>
				<less-than>
					<property>sim/multiplay/generic/float[0]</property>
					<value>0.1</value>
				</less-than>
			</condition>
			<pitch>
				<property>engines/engine/n2</property>
				<factor>0.01</factor>
				<min>0</min>
				<max>0.9</max>
			</pitch>
			<volume>
				<property>engines/engine/n2</property>
				<factor>0.05</factor>
				<min>0.0</min>
				<max>0.5</max>
			</volume>
		</engine>
		<engine>
			<name>external-afterburner</name>
			<mode>looped</mode>
			<path>Sounds/engine/afterburner.wav</path>
			<condition>
				<property>engines/engine[0]/augmentation</property>
				<not>
					<property>engines/engine[0]/out-of-fuel</property>
				</not>
				<not>
					<and>
						<property>sim/current-view/internal</property>
						<less-than>
							<property>sim/multiplay/generic/float[0]</property>
							<value>0.1</value>
						</less-than>
					</and>
				</not>
			</condition>
			<orientation>
				<x>0.0</x>
				<y>0.0</y>
				<z>180.0</z>
				<inner-cone>180.0</inner-cone>
				<outer-cone>360.0</outer-cone>
				<outer-gain>0.2</outer-gain>
			</orientation>
		</engine>
		<engine>
			<name>internal-afterburner</name>
			<mode>looped</mode>
			<path>Sounds/exhaust_loop.wav</path>
			<condition>
				<property>engines/engine[0]/augmentation</property>
				<not>
					<property>engines/engine[0]/out-of-fuel</property>
				</not>
				<property>sim/current-view/internal</property>
				<less-than>
					<property>sim/multiplay/generic/float[0]</property>
					<value>0.1</value>
				</less-than>
			</condition>
			<volume>
				<max>0.5</max>
			</volume>
		</engine>
		<engine>
			<name>engine-shutdown</name>
			<mode>once</mode>
			<path>Sounds/turbine_shutdown.wav</path>
			<condition>
				<and>
					<property>/controls/engines/engine[0]/cutoff</property>
					<greater-than>
						<property>/controls/engines/engine[0]/n1</property>
						<value>0</value>
					</greater-than>
				</and>
			</condition>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</engine>
		<engine>
			<name>wind</name>
			<mode>looped</mode>
			<path>Sounds/misc/BaBoOn-mirageIV-Wind.wav</path>
			<condition>
				<not>
					<property>/sim/current-view/internal</property>
				</not>
			</condition>
			<property>/velocities/airspeed-kt</property>
			<volume>
				<property>/position/altitude-ft</property>
				<factor>-0.000015</factor>
				<offset>1.0</offset>
				<min>0.1</min>
				<max>0.7</max>
			</volume>
			<volume>
				<property>/velocities/airspeed-kt</property>
				<factor>0.015</factor>
				<min>0.03</min>
				<max>0.55</max>
			</volume>
			<pitch>
				<property>/velocities/airspeed-kt</property>
				<factor>0.01</factor>
				<offset>0.75</offset>
			</pitch>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</engine>
		<!--================================= Rain =============================-->
		<rain>
			<name>rain_ext</name>
			<mode>looped</mode>
			<path>Sounds/misc/rain.wav</path>
			<condition>
				<not>
					<property>sim/current-view/internal</property>
				</not>
				<greater-than>
					<property>/environment/rain-norm</property>
					<value>0</value>
				</greater-than>
			</condition>
			<volume>
				<property>/environment/rain-norm</property>
				<factor>0.40</factor>
			</volume>
		</rain>
		<rain>
			<name>rain_int1</name>
			<mode>looped</mode>
			<path>Sounds/misc/rain.wav</path>
			<type>avionics</type>
			<condition>
				<property>sim/current-view/internal</property>
				<greater-than>
					<property>/environment/rain-norm</property>
					<value>0</value>
				</greater-than>
			</condition>
			<volume>
				<property>/environment/rain-norm</property>
				<factor>0.18</factor>
			</volume>
		</rain>
		<rain>
			<name>rain_int2</name>
			<mode>looped</mode>
			<path>Sounds/misc/rain-windshield.wav</path>
			<type>avionics</type>
			<condition>
				<property>sim/current-view/internal</property>
				<greater-than>
					<property>/environment/rain-norm</property>
					<value>0</value>
				</greater-than>
			</condition>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.0075</factor>
				<min>0.06</min>
				<max>0.40</max>
			</volume>
		</rain>
		<!--================================= Avionics =============================-->
		<avionics>
			<name>inverter</name>
			<path>Sounds/misc/el_loop.wav</path>
			<mode>looped</mode>
			<type>avionics</type>
			<condition>
				<property>sim/current-view/internal</property>
				<property>fdm/jsbsim/electric/sources/output-dc</property>
			</condition>
			<volume>
				<factor>0.5</factor>
			</volume>
		</avionics>
		<stall>
			<name>stall</name>
			<mode>once</mode>
			<path>Sounds/misc/RitaHighAoA.wav</path>
			<condition>
				<greater-than>
					<property>orientation/alpha-deg</property>
					<value>18</value>
				</greater-than>
				<not>
					<property>/gear/gear[0]/wow</property>
					<property>/gear/gear[1]/wow</property>
					<property>/gear/gear[2]/wow</property>
					<property>/gear/gear[0]/position-norm</property>
				</not>
			</condition>
			<volume>
				<factor>1.0</factor>
			</volume>
			<position>
				<x>6</x>
				<y>0</y>
				<z>1</z>
			</position>
			<reference-dist>1.0</reference-dist>
			<max-dist>8.0</max-dist>
		</stall>
		<avionics>
			<name>masterwarn_buzz</name>
			<path>Sounds/misc/mwarn.wav</path>
			<mode>looped</mode>
			<type>avionics</type>
			<condition>
				<property>/fdm/jsbsim/warning-lights/masterwarn</property>
				<property>sim/current-view/internal</property>
			</condition>
			<volume>
				<factor>0.8</factor>
			</volume>
		</avionics>
		<avionics>
			<name>Clickon</name>
			<path>Sounds/click.wav</path>
			<condition>
				<property>sim/model/click</property>
			</condition>
			<volume>
				<factor>1.0</factor>
				<min>1.0</min>
				<max>1.0</max>
			</volume>
		</avionics>
		<avionics>
			<name>Clickoff</name>
			<path>Sounds/click.wav</path>
			<type>avionics</type>
			<condition>
				<not>
					<property>sim/model/click</property>
				</not>
			</condition>
			<volume>
				<factor>1.0</factor>
			</volume>
		</avionics>
		<!--================================= Gears =============================-->
		<gears>
			<name>gear</name>
			<mode>in-transit</mode>
			<path>Sounds/misc/Mirage2000_gear2.wav</path>
			<type>avionics</type>
			<condition>
				<greater-than>
					<property>gear/gear[0]/position-norm</property>
					<value>0</value>
				</greater-than>
				<less-than>
					<property>gear/gear[0]/position-norm</property>
					<value>1</value>
				</less-than>
				<equals>
					<property>controls/engines/engine[0]/cutoff</property>
					<value>0</value>
				</equals>
				<property>sim/current-view/internal</property>
			</condition>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</gears>
		<gears>
			<name>gear-open-door</name>
			<!-- <mode>in-transit</mode>-->
			<path>Sounds/misc/Mirage2000_Open_gear_Door.wav</path>
			<type>avionics</type>
			<condition>
				<greater-than>
					<property>/gear/gear[0]/position-norm</property>
					<value>0</value>
				</greater-than>
				<less-than>
					<property>/gear/gear[0]/position-norm</property>
					<value>1</value>
				</less-than>
				<property>sim/current-view/internal</property>
			</condition>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</gears>
		<gears>
			<name>gear-close-door</name>
			<path>Sounds/misc/Mirage2000_Close_gear_Door.wav</path>
			<type>avionics</type>
			<condition>
				<or>
					<less-than>
						<property>/gear/gear[0]/position-norm</property>
						<value>0.1</value>
					</less-than>
					<greater-than>
						<property>/gear/gear[0]/position-norm</property>
						<value>0.9</value>
					</greater-than>
				</or>
				<greater-than>
					<property>/gear/gear[0]/position-norm</property>
					<value>0</value>
				</greater-than>
				<less-than>
					<property>/gear/gear[0]/position-norm</property>
					<value>1</value>
				</less-than>
				<property>sim/current-view/internal</property>
			</condition>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</gears>
		<gears>
			<name>rattleroll</name>
			<mode>looped</mode>
			<path>Sounds/rattleroll.wav</path>
			<type>avionics</type>
			<condition>
				<or>
					<property>gear/gear[0]/wow</property>
					<property>gear/gear[1]/wow</property>
					<property>gear/gear[2]/wow</property>
				</or>
				<property>sim/current-view/internal</property>
			</condition>
			<volume>
				<property>gear/gear[0]/rollspeed-ms</property>
				<factor>0.1</factor>
				<max>5.0</max>
			</volume>
		</gears>
		<gears>
			<name>squeal</name>
			<path>Sounds/BaBoOn-mirageIV-Tire-Squeal.wav</path>
			<type>avionics</type>
			<condition>
				<or>
					<property>/gear/gear[0]/wow</property>
					<property>/gear/gear[1]/wow</property>
					<property>/gear/gear[2]/wow</property>
				</or>
			</condition>
			<volume>
				<property>/velocities/speed-down-fps</property>
				<factor>0.05</factor>
			</volume>
			<volume>
				<property>/velocities/airspeed-kt</property>
				<factor>0.01</factor>
			</volume>
			<volume>
				<internal>dt_stop</internal>
				<factor>0.05</factor>
				<max>1.0</max>
			</volume>
			<pitch>
				<property>/velocities/airspeed-kt</property>
				<factor>0.0025</factor>
				<offset>1.2</offset>
			</pitch>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</gears>
		<!--================================= Flaps =============================-->
		<flaps>
			<name>flaps</name>
			<mode>in-transit</mode>
			<path>Sounds/flaps.wav</path>
			<type>avionics</type>
			<property>surface-positions/flap-pos-norm</property>
			<volume>
				<max>0.7</max>
			</volume>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</flaps>
		<!--================================= Guns =============================-->
		<guns>
			<name>gunfire</name>
			<mode>looped</mode>
			<path>Sounds/weapons/M-2000-Guns.wav</path>
			<condition>
				<greater-than>
					<property>ai/submodels/submodel[8]/count</property>
					<value>0</value>
				</greater-than>
				<property>fdm/jsbsim/fcs/guntrigger</property>
			</condition>
			<volume>
				<value>1</value>
			</volume>
		</guns>
		<guns>
			<name>upk-23 gunfire 1</name>
			<mode>looped</mode>
			<path>Sounds/weapons/M-2000-Guns.wav</path>
			<condition>
				<greater-than>
					<property>ai/submodels/submodel[10]/count</property>
					<value>0</value>
				</greater-than>
				<property>fdm/jsbsim/fcs/upk-23-trigger1</property>
			</condition>
			<volume>
				<value>1</value>
			</volume>
		</guns>
		<guns>
			<name>upk-23 gunfire 2</name>
			<mode>looped</mode>
			<path>Sounds/weapons/M-2000-Guns.wav</path>
			<condition>
				<greater-than>
					<property>ai/submodels/submodel[12]/count</property>
					<value>0</value>
				</greater-than>
				<property>fdm/jsbsim/fcs/upk-23-trigger2</property>
			</condition>
			<volume>
				<value>1</value>
			</volume>
		</guns>
		<!--================================= Missiles =============================-->
		<missiles>
			<name>R-3S lock</name>
			<path>Sounds/weapons/missile-lock.wav</path>
			<mode>looped</mode>
			<condition>
				<property>payload/armament/r-3s/sound-volume</property>
			</condition>
			<volume>
				<factor>0.75</factor>
			</volume>
		</missiles>
		<missiles>
			<name>R-3S</name>
			<path>Sounds/weapons/missile.wav</path>
			<mode>once</mode>
			<type>avionics</type>
			<condition>
				<property>payload/armament/r-3s/sound-fire-on-off</property>
			</condition>
			<volume>
				<factor>0.75</factor>
			</volume>
		</missiles>
		<missiles>
			<name>R-13M lock</name>
			<path>Sounds/weapons/missile-lock.wav</path>
			<mode>looped</mode>
			<condition>
				<property>payload/armament/r-13m/sound-volume</property>
			</condition>
			<volume>
				<factor>0.75</factor>
			</volume>
		</missiles>
		<missiles>
			<name>R-13M</name>
			<path>Sounds/weapons/missile.wav</path>
			<mode>once</mode>
			<condition>
				<property>payload/armament/r-13m/sound-fire-on-off</property>
			</condition>
			<volume>
				<factor>0.75</factor>
			</volume>
		</missiles>
		<missiles>
			<name>R-60M lock</name>
			<path>Sounds/weapons/missile-lock.wav</path>
			<mode>looped</mode>
			<condition>
				<property>payload/armament/r-60m/sound-volume</property>
			</condition>
			<volume>
				<factor>0.75</factor>
			</volume>
		</missiles>
		<missiles>
			<name>R-60M</name>
			<path>Sounds/weapons/missile.wav</path>
			<mode>once</mode>
			<condition>
				<property>payload/armament/r-60m/sound-fire-on-off</property>
			</condition>
			<volume>
				<factor>0.75</factor>
			</volume>
		</missiles>
		<missiles>
			<name>R-73 lock</name>
			<path>Sounds/weapons/missile-lock.wav</path>
			<mode>looped</mode>
			<condition>
				<property>payload/armament/r-73/sound-volume</property>
			</condition>
			<volume>
				<factor>0.75</factor>
			</volume>
		</missiles>
		<missiles>
			<name>R-73</name>
			<path>Sounds/weapons/missile.wav</path>
			<mode>once</mode>
			<condition>
				<property>payload/armament/r-73/sound-fire-on-off</property>
			</condition>
			<volume>
				<factor>0.75</factor>
			</volume>
		</missiles>
		<!--================================= Bombs =============================-->
		<bombs>
			<name>FAB-500</name>
			<mode>once</mode>
			<path>Sounds/weapons/bomb_away.wav</path>
			<condition>
				<property>payload/armament/fab-500/sound-fire-on-off</property>
			</condition>
			<volume>
				<property>sim/current-view/internal</property>
				<factor>1</factor>
				<max>1</max>
			</volume>
		</bombs>
		<bombs>
			<name>RBK-500</name>
			<mode>once</mode>
			<path>Sounds/weapons/bomb_away.wav</path>
			<condition>
				<property>payload/armament/rbk-500/sound-fire-on-off</property>
			</condition>
			<volume>
				<property>sim/current-view/internal</property>
				<factor>1</factor>
				<max>1</max>
			</volume>
		</bombs>
		<!--================================= Explosions =============================-->
		<explosions>
			<name>nearby-explode</name>
			<path>Sounds/weapons/aircraft-explode.wav</path>
			<mode>once</mode>
			<condition>
				<equals>
					<property>damage/sounds/nearby-explode-on</property>
					<value>1</value>
				</equals>
			</condition>
			<position>
				<x>0.01</x>
				<y>0.01</y>
				<z>0.01</z>
			</position>
			<pitch>
				<factor>1.05</factor>
			</pitch>
			<volume>
				<factor>1</factor>
				<max>1</max>
			</volume>
		</explosions>
	</fx>
</PropertyList>
