 <animation>
  <type>translate</type>
  <object-name>PNP.Button</object-name>
  <property>mig29/controls/PNP-72-12/test</property>
  <factor>0.003</factor>
   <axis>
   <x>-1</x>
   <y> 0</y>
   <z> 0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>PNP.knobSetPathAngle</object-name>
  <property>mig29/controls/PNP-72-12/ZPU</property>
  <factor>5</factor>
  <center>
   <x-m> 0.0000</x-m>
   <y-m> 0.0440</y-m>
   <z-m>-0.0482</z-m>
   </center>
   <axis>
   <x>-1</x>
   <y> 0</y>
   <z> 0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>PNP.cz1</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/PNP-72-12/test</property>
    <value>1</value>
   </binding>
    <mod-up>
    <binding>
     <command>property-assign</command>
     <property>mig29/controls/PNP-72-12/test</property>
     <value>0</value>
    </binding>
   </mod-up>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>PNP.cz2</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>true</repeatable>
    <binding>
    <command>property-adjust</command>
    <property>mig29/controls/PNP-72-12/ZPU</property>
    <step>-1</step>
    <min>-3600</min>
    <max>3590</max>
    </binding>
  </action>
 </animation>
 <animation>
  <type>pick</type>
  <object-name>PNP.cz3</object-name>
  <visible>false</visible>
  <action>
   <button>0</button>
   <repeatable>true</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>mig29/controls/PNP-72-12/ZPU</property>
    <step>1</step>
    <min>-3600</min>
    <max>3590</max>
   </binding>
  </action>
 </animation>

</PropertyList>
