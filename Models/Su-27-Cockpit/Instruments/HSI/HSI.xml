<animation>
  <type>rotate</type>
  <object-name>Compass</object-name>
    <property>orientation/heading-magnetic-deg</property>
  <center>
   <x-m> 0.0</x-m>
   <y-m> 0.0</y-m>
   <z-m>-0.0085</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

  <animation>
    <type>rotate</type>
    <object-name>Course</object-name>
    <expression>
      <difference>
		<property>instrumentation/nav[0]/radials/selected-deg</property>
        <property>orientation/heading-magnetic-deg</property>
      </difference>
    </expression>
  <center>
   <x-m> 0.0</x-m>
   <y-m> 0.0</y-m>
   <z-m>-0.0085</z-m>
  </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>RadioCompass</object-name>
    <expression>
      <difference>
        <property>instrumentation/nav[0]/heading-deg</property>
        <property>orientation/heading-magnetic-deg</property>
      </difference>
    </expression>
  <center>
   <x-m> 0.0</x-m>
   <y-m> 0.0</y-m>
   <z-m>-0.0085</z-m>
