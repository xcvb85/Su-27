<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<path>Su-33.ac</path>
	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>GLS_Canopy_Front</object-name>
		<object-name>GLS_Canopy_Rear</object-name>
	</effect>
	<effect>
		<name>Aircraft/Su-27/Models/Effects/fabric</name>
		<inherits-from>Aircraft/Su-27/Models/Effects/fabric</inherits-from>
		<object-name>HULL_Hull_Nose.002</object-name>
	</effect>
	<model>
		<name>cockpit</name>
		<path>Aircraft/Su-27/Models/Su-27-Cockpit/Su-27-Cockpit.xml</path>
		<offsets>
			<x-m> 0.3</x-m>
			<y-m> 0.0</y-m>
			<z-m> 0.0</z-m>
			<pitch-deg>0</pitch-deg>
		</offsets>
	</model>
	<model>
		<name>Stores</name>
		<path>Aircraft/Su-27/Models/Stores/Stores.xml</path>
	</model>
	<animation>
		<type>select</type>
		<object-name>HULL_Nose_WWell</object-name>
		<object-name>HULL_Nose_Door_Front_Int</object-name>
		<object-name>HULL_Nose_Door_Rear_Int</object-name>
		<object-name>UC_Nose_Upper_Strut</object-name>
		<object-name>UC_Nose_Lower_Strut</object-name>
		<object-name>UC_Nose_LLights</object-name>
		<object-name>UC_Nose_Upright_Brace</object-name>
		<object-name>UC_Nose_WGuard</object-name>
		<object-name>UC_Nose_Tyres</object-name>
		<object-name>UC_Nose_Hubs</object-name>
		<object-name>UC_Nose_BSArm</object-name>
		<object-name>UC_Nose_Rear_Post</object-name>
		<condition>
		<greater-than>
			<property>gear/gear[0]/position-norm</property>
			<value>0.3</value>
		</greater-than>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>HULL_LM_WWell</object-name>
		<object-name>HULL_LMR_Door_Int</object-name>
		<object-name>HULL_LMF_Door_Int</object-name>
		<object-name>UC_Main_Left_Upper_St</object-name>
		<object-name>UC_Main_Left_Tyre</object-name>
		<object-name>UC_Main_Left_Hub</object-name>
		<object-name>UC_Main_Left_Upper_DA</object-name>
		<object-name>UC_Main_Left_Lower_DA</object-name>
		<condition>
		<greater-than>
			<property>gear/gear[1]/position-norm</property>
			<value>0.3</value>
		</greater-than>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>HULL_RM_WWell</object-name>
		<object-name>HULL_RMR_Door_Int</object-name>
		<object-name>HULL_RMF_Door_Int</object-name>
		<object-name>UC_Main_Right_Upper_S</object-name>
		<object-name>UC_Main_Right_Tyre</object-name>
		<object-name>UC_Main_Right_Hub</object-name>
		<object-name>UC_Main_Right_Upper_D</object-name>
		<object-name>UC_Main_Right_Lower_D</object-name>
		<condition>
		<greater-than>
			<property>gear/gear[2]/position-norm</property>
			<value>0.3</value>
		</greater-than>
		</condition>
	</animation>
</PropertyList>
